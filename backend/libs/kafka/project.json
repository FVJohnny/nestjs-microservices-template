{
  "name": "nestjs-kafka",
  "sourceRoot": "backend/libs/kafka/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": [
        "{projectRoot}/dist"
      ],
      "options": {
        "command": "npm run build",
        "cwd": "backend/libs/kafka"
      },
      "dependsOn": ["^build"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "backend/libs/kafka/src/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "backend/libs/kafka/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": [
    "type:lib",
    "scope:nestjs",
    "topic:kafka"
  ],
  "implicitDependencies": ["nestjs-common"]
}