{
  "name": "nestjs-kafka",
  "sourceRoot": "backend/libs/nestjs/kafka/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "outputPath": "backend/libs/nestjs/kafka/dist",
        "main": "backend/libs/nestjs/kafka/src/index.ts",
        "tsConfig": "backend/libs/nestjs/kafka/tsconfig.json",
        "assets": []
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["backend/libs/nestjs/kafka/src/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "backend/libs/nestjs/kafka/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["type:lib", "scope:nestjs", "topic:kafka"]
}