{
  "name": "nestjs-redis",
  "sourceRoot": "backend/libs/nestjs/redis/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "outputPath": "backend/libs/nestjs/redis/dist",
        "main": "backend/libs/nestjs/redis/src/index.ts", 
        "tsConfig": "backend/libs/nestjs/redis/tsconfig.json",
        "assets": []
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["backend/libs/nestjs/redis/src/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "backend/libs/nestjs/redis/jest.config.js"
      }
    }
  },
  "tags": ["type:lib", "scope:nestjs", "topic:redis"]
}