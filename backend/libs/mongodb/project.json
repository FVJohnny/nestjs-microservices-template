{
  "name": "nestjs-mongodb",
  "sourceRoot": "backend/libs/mongodb/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{projectRoot}/dist"
      ],
      "options": {
        "outputPath": "backend/libs/mongodb/dist",
        "main": "backend/libs/mongodb/src/index.ts",
        "tsConfig": "backend/libs/mongodb/tsconfig.json",
        "assets": []
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "backend/libs/mongodb/src/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "backend/libs/mongodb/jest.config.js"
      }
    }
  },
  "tags": [
    "type:lib",
    "scope:nestjs",
    "topic:mongodb"
  ]
}