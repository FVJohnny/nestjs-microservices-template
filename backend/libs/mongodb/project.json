{
  "name": "nestjs-mongodb",
  "sourceRoot": "backend/libs/mongodb/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "npm run build",
        "cwd": "backend/libs/mongodb"
      },
      "dependsOn": ["^build"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["backend/libs/mongodb/src/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "backend/libs/mongodb/jest.config.js"
      }
    }
  },
  "tags": ["type:lib", "scope:nestjs", "topic:mongodb"],
  "implicitDependencies": ["nestjs-common"]
}
