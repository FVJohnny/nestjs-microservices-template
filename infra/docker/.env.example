# Production Cloud Configuration Override
# Copy this file to .env and uncomment/modify values to connect to cloud resources
# If .env doesn't exist, docker-compose.prod.yml will use local defaults

# Kafka Configuration
# KAFKA_BROKERS=your_kafka_host:9092
# KAFKA_USERNAME=your_kafka_username
# KAFKA_PASSWORD=your_kafka_password

# MongoDB Configuration
# MONGODB_URI=mongodb://your_mongodb_username:your_mongodb_password@your_mongodb_host:27017/your_database_name
# MONGODB_DB_NAME=your_database_name

# Redis Configuration
# REDIS_HOST=your_redis_host
# REDIS_PORT=6379
# REDIS_USERNAME=your_redis_username
# REDIS_PASSWORD=your_redis_password
# REDIS_DB=0
# REDIS_TLS=false

# PostgreSQL Configuration
# POSTGRES_HOST=your_postgres_host
# POSTGRES_PORT=5432
# POSTGRES_USER=your_postgres_user
# POSTGRES_PASSWORD=your_postgres_password
# POSTGRES_DB=your_database_name
# POSTGRES_SYNCHRONIZE=false
# POSTGRES_LOGGING=false

# Other Production Settings
# CORS_ORIGINS=https://yourdomain.com,https://www.yourdomain.com